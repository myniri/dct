{
  "HackerNewsOptions": {
    "BaseUrl": "https://hacker-news.firebaseio.com/v0/",
    "BestStoriesEndpoint": "beststories.json",
    "ItemEndpoint": "item/{0}.json",
    "CacheResponseForSeconds": 300
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}